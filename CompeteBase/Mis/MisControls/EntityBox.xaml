<UserControl x:Class="Compete.Mis.MisControls.EntityBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="22" d:DesignWidth="600">
    <DockPanel>
        <Button Name="SelectButton" Content="..." DockPanel.Dock="Right" Width="22"/>
        <Button Name="ClearButton" Content="C" DockPanel.Dock="Right" Width="22"/>
        <TextBox Name="DisplayTextBox" IsReadOnly="True"/>
    </DockPanel>
</UserControl>
