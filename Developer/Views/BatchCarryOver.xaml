<UserControl x:Class="Compete.Mis.Developer.Views.BatchCarryOver"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:Compete.Mis.MisControls;assembly=CompeteBase"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,14,0,0" Text="{DynamicResource BatchCarryOver.BeginYearMonth}" VerticalAlignment="Top"/>
        <controls:YearMonthPicker HorizontalAlignment="Left" Margin="63,10,0,0" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,43,0,0" Text="{DynamicResource BatchCarryOver.EndYearMonth}" VerticalAlignment="Top"/>
        <controls:YearMonthPicker HorizontalAlignment="Left" Margin="63,39,0,0" VerticalAlignment="Top"/>
        <Button Content="{DynamicResource BatchCarryOver.CarryOver}" HorizontalAlignment="Left" Margin="63,68,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</UserControl>
